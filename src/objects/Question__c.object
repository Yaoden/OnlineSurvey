    <validationRules>
        <fullName>Exam_question_cannot_be_short_answer</fullName>
        <active>true</active>
        <description>If a question is for an exam, the question type cannot be short answer</description>
        <errorConditionFormula>AND(ISPICKVAL(Questionfor__c,&apos;Exam&apos;), RecordType.Name = &apos;Short Answer&apos;)</errorConditionFormula>
        <errorDisplayField>Questionfor__c</errorDisplayField>
        <errorMessage>Exam question cannot be a short answer</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Exam_question_has_correct_answer</fullName>
        <active>true</active>
        <description>The exam question must contain the correct answer</description>
        <errorConditionFormula>AND(ISPICKVAL(Questionfor__c,&apos;Exam&apos;),
ISBLANK(CorrectAnswer__c),
 RecordType.Name &lt;&gt; &apos;Short Answer&apos;
)</errorConditionFormula>
        <errorDisplayField>CorrectAnswer__c</errorDisplayField>
        <errorMessage>You must provide the correct answer for the exam question</errorMessage>
    </validationRules>
